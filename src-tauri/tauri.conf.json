{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Open WebUI Lite Desktop",
  "version": "0.9.3",
  "identifier": "com.github.xxnuo.owld",
  "mainBinaryName": "Open WebUI Lite Desktop",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "./static"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Open WebUI Lite Desktop",
        "width": 1440,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "deb",
      "rpm",
      "app",
      "dmg",
      "nsis",
      "msi"
    ],
    "externalBin": [
      "../bin/open-webui-lite"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}