{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "open-webui-lite-desktop",
  "version": "0.1.0",
  "identifier": "com.github.xxnuo.owld",
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "open-webui-lite-desktop",
        "width": 1440,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "rpm"],
    "externalBin": [
      "../build/open-webui-lite"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}